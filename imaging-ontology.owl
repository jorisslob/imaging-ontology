<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY prov "http://www.w3.org/ns/prov#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#"
     xml:base="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:prov="http://www.w3.org/ns/prov#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology">
        <owl:versionIRI rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology/0.5.0"/>
        <owl:imports rdf:resource="http://www.w3.org/ns/prov-o-20130430"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasImagingMode -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasImagingMode">
        <rdfs:range rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Imaging_Mode"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Microscope"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasImmersionMedium -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasImmersionMedium">
        <rdfs:range rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Immersion_Medium"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Microscope"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasInteraction -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasInteraction">
        <rdfs:range rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Interaction"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Microscope"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasManufacturer -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasManufacturer">
        <rdfs:range rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Commercial_Company"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Microscope"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#emissionWavelength -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#emissionWavelength">
        <rdfs:comment xml:lang="en">Value in nm</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Fluorescent_Microscope"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#excitationWavelength -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#excitationWavelength">
        <rdfs:comment xml:lang="en">Value in nm</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Fluorescent_Microscope"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasOpticalResolution -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasOpticalResolution">
        <rdfs:comment xml:lang="en">value in nm</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Optical_Microscope"/>
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasResolution"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasResolution -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasResolution">
        <rdfs:comment xml:lang="en">value in nm</rdfs:comment>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Microscope"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#numericalAperture -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#numericalAperture">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Optical_Microscope"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#pinholeSize -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#pinholeSize">
        <rdfs:comment xml:lang="en">value in micrometers</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;decimal"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasImagingMode"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Confocal"/>
            </owl:Restriction>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Aberration_Correction -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Aberration_Correction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Objective_Designation"/>
        <prov:definition xml:lang="en">Chromatic aberration correction (achromatic, fluorite or apochromatic) characteristic of an objective</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Air -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Air">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Immersion_Medium"/>
        <prov:definition xml:lang="en">Mixture of gasses found in the atmosphere of Earth.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Atomic_Force_Microscope -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Atomic_Force_Microscope">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasImagingMode"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Scanning_Probe"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasInteraction"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Mechanical_Force"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Microscope"/>
        <prov:definition xml:lang="en">An Atomic Force Microscope or Scanning Force Microscope is a very high-resolution type of Scanning Probe Microscope, with demonstrated resolution on the order of fractions of a nanometer, more than 1000 times better than the optical diffraction limit.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Bright_Field -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Bright_Field">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Imaging_Mode"/>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://bioontology.org/projects/ontologies/birnlex#birnlex_2255</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://dbpedia.org/page/Bright_field_microscopy</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://ontology.neuinfo.org/NIF/DigitalEntities/NIF-Investigation.owl#birnlex_2255</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.bioontology.org/ontology/RCD/X79me</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.bioontology.org/ontology/SNOMEDCT/257425005</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.obolibrary.org/obo/CHMO_0000104</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.obolibrary.org/obo/FBbi_00000243</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://www.owl-ontologies.com/Ontology1298855822.owl#Bright-field</rdfs:seeAlso>
        <prov:definition xml:lang="en">Simplest of all the optical microscopy illumination techniques.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Bright_Field_Microscope -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Bright_Field_Microscope">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasImagingMode"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Bright_Field"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasInteraction"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Photon_Absorbance"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Optical_Microscope"/>
        <prov:definition xml:lang="en">A Bright Field Microscope is the simplest of all the Optical Microscopes.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Commercial_Company -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Commercial_Company">
        <rdfs:subClassOf rdf:resource="&prov;Organization"/>
        <prov:definition xml:lang="en">An organization that is involved in commercialization of detection instruments</prov:definition>
        <rdfs:comment xml:lang="en">Same use as BAO (BioAssayOntology)</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Confocal -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Confocal">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Imaging_Mode"/>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://dbpedia.org/page/Confocal_microscopy</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://dicom.nema.org/resources/ontology/DCM/114207</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#Confocal_Microscopy</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://phenomebrowser.net/ontologies/mesh/mesh.owl#D018613</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.bioontology.org/ontology/MESH/D018613</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.obolibrary.org/obo/ERO_0000200</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.obolibrary.org/obo/FBbi_00000251</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://www.bioassayontology.org/bao#BAO_0000453</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://www.projecthalo.com/aura#Confocal-Microscopy</rdfs:seeAlso>
        <prov:definition xml:lang="en">Optical imaging technique used to increase optical resolution and contrast of a micrograph by adding a spatial pinhole placed at the confocal plane of the lens to eliminate out-of-focus light.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Confocal_Microscope -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Confocal_Microscope">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasImagingMode"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Confocal"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasInteraction"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Fluorescence"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Fluorescent_Microscope"/>
        <prov:definition xml:lang="en">A Confocal Microscope uses an optical imaging technique used to increase optical resolution and contrast of a micrograph by adding a spatial pinhole placed at the confocal plane of the lens to eliminate out-of-focus light.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Device -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Device">
        <rdfs:subClassOf rdf:resource="&prov;Agent"/>
        <prov:definition xml:lang="en">A constructed tool used in an experiment</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Absorbance -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Absorbance">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Interaction"/>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://en.wikipedia.org/wiki/Transmission_electron_microscopy</rdfs:seeAlso>
        <prov:definition xml:lang="en">The absorption of electrons is the way in which electrons are taken up by matter.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Excitation -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Excitation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Interaction"/>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://dbpedia.org/page/Electron_excitation</rdfs:seeAlso>
        <prov:definition xml:lang="en">Emission of secondary electrons by atoms excited by a primary electron beam.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Interaction -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Interaction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Interaction"/>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://www.ammrf.org.au/myscope/sem/background/concepts/interactions.php</rdfs:seeAlso>
        <prov:definition xml:lang="en">An interaction that has at least some electrons as a component.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Microscope -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Microscope">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#hasInteraction"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Electron_Interaction"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Microscope"/>
        <prov:definition xml:lang="en">An Electron Microscope is a Microscope that uses accelerated electrons as a source of illumination.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Epifluorescence -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Epifluorescence">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Imaging_Mode"/>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">Epifluorescence illumination method for specimen microscopy.</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.bioontology.org/ontology/SNOMEDCT/698790009</rdfs:seeAlso>
        <prov:definition xml:lang="en">Excitation of the fluorophore and detection of the fluorescence are done through the same light path (i.e. through the objective).</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Evanescent_Waves -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Evanescent_Waves">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Photon_Interaction"/>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://dbpedia.org/page/Evanescent_wave</rdfs:seeAlso>
        <prov:definition xml:lang="en">An evanescent wave is a near-field wave with an intensity that exhibits exponential decay without absorption as a function of the distance from the boundary at which the wave was formed.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Experiment -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Experiment">
        <rdfs:subClassOf rdf:resource="&prov;Activity"/>
        <prov:definition xml:lang="en">An experiment in this context is a process using at least one device that results in a measurement.</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Flat-Field_Correction -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Flat-Field_Correction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Objective_Designation"/>
        <prov:definition xml:lang="en">Field curvature correction characteristic of an objective</prov:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Fluorescence -->

    <owl:Class rdf:about="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Fluorescence">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/jslob/ontologies/2014/7/imaging-ontology#Photon_Interaction"/>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://dbpedia.org/page/Fluorescence</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C16586</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://phenomebrowser.net/ontologies/mesh/mesh.owl#G01.358.500.505.650.665.500</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.bioontology.org/ontology/CSP/2138-3281</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.bioontology.org/ontology/MESH/D005453</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.bioontology.org/ontology/RCD/X7A1Y</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.bioontology.org/ontology/SNOMEDCT/258099003</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.obolibrary.org/obo/ID_0000053</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.obolibrary.org/obo/PATO_0000018</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://purl.obolibrary.org/obo/REX_0000043</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://sweet.jpl.nasa.gov/2.3/procChemical.owl#Fluorescence</rdfs:seeAlso>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI">http://www.projecthalo.com/aura#Fluorescence</rdfs:seeAlso>
        <prov:definition xml:lang="en">Emission of light by a substance that has absorbed light or other electomagnetic radiation.</prov:definition>
    </owl:Class>
    


